#lang racket

(require racket/contract)


;;ZAD 1 -------------------------------------------------------------------------

(define (suffixes xs)
  (parametric->/c [a] (-> (listof a) (listof (cdr a))))
  (if (null? xs)
      xs
      (cons xs (suffixes (cdr xs)))))

(suffixes (list 1 2 3))


